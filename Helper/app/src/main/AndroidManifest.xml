<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.destroy.helper">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.destroy.newstec.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.destroy.newstec.All_Paper_Activity" android:parentActivityName="com.example.destroy.newstec.MainActivity" />
        <activity android:name="com.example.destroy.newstec.Govment_Activity" />
        <activity android:name="com.example.destroy.newstec.ProgrammingActivity" />
        <activity android:name="com.example.destroy.newstec.EntertainmentActivity" />
        <activity android:name="com.example.destroy.newstec.HealthActivity" android:parentActivityName="com.example.destroy.newstec.MainActivity"/>
        <activity android:name="com.example.destroy.newstec.Bangladeshi_papers" android:parentActivityName="com.example.destroy.newstec.All_Paper_Activity"/>
        <activity android:name="com.example.destroy.newstec.World_famous_paper" android:parentActivityName="com.example.destroy.newstec.All_Paper_Activity"/>
        <activity android:name="com.example.destroy.newstec.Show_all" android:parentActivityName="com.example.destroy.newstec.Show_all"/>
        <activity android:name="com.example.destroy.newstec.show_world_paper" android:parentActivityName="com.example.destroy.newstec.World_famous_paper" />
        <activity android:name="com.example.destroy.newstec.Aboutme" />
        <activity android:name="com.example.destroy.newstec.HelpOfphone"></activity>
    </application>

</manifest>